			path="autheticationFailed" 
			scope="request" 
			bundle="ExceptionResources" />
		<exception type="cl.araucana.ea.struts.exceptions.DuplicateFormSubmissionException" 
			handler="cl.araucana.ea.struts.exceptionHandlers.DuplicateFormSubmissionExceptionHandler" 
