<!--	Integración con Sucursal Virtual (gpavez@hotmail.com, 01/11/2007)
		<forward name="logout" path="/ibm_security_logout?logoutExitPage=/proxy.do" />
-->
		<forward name="logout" path="/logout.do" />
		
		<forward name="defaultError" path="/pages/error/default.jsp" />
		<forward name="authenticationFailed" path="/pages/error/authenticationFailed.jsp" />
		<forward name="duplicateFormSubmission" path="/pages/error/duplicateFormSubmission.jsp" />
		<forward name="invalidParameter" path="/pages/error/invalidParameter.jsp" />

<!--	Integración con Sucursal Virtual (gpavez@hotmail.com, 01/11/2007)
		<forward name="invalidProfile" path="/ibm_security_logout?logoutExitPage=/pages/error/invalidProfile.jsp" />
		<forward name="invalidSessionStatus" path="/ibm_security_logout?logoutExitPage=/pages/error/invalidSessionStatus.jsp" />
-->
		<forward name="invalidProfile" path="/logout.do" />
		<forward name="invalidSessionStatus" path="/logout.do" />

		<forward name="serviceUnavailable" path="/pages/error/serviceUnavailable.jsp" />
		<forward name="unauthorizedAccess" path="/pages/error/unauthorizedAccess.jsp" />
