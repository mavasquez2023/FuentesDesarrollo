<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name>AltasBajasIMED</display-name>
	<servlet>
    <servlet-name>LoadOnStartupQuartz</servlet-name>
    <servlet-class>cl.laaraucana.imed.jcrontab.InitQuartz</servlet-class>
		<init-param>
			<param-name>log4j-init-file</param-name>
			<param-value>WEB-INF/log4j.properties</param-value>
		</init-param>
    <load-on-startup>10</load-on-startup>
  </servlet>
  <servlet>
		<servlet-name>runImed</servlet-name>
		<servlet-class>cl.laaraucana.imed.servlet.ImedRun</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>runImed</servlet-name>
		<url-pattern>/runImed</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>