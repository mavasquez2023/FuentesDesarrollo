/* NRP10F1 */


ALTER TABLE NRPDTA.NRP10F1 ADD COLUMN FRENOVACION  VARCHAR(8);
COMMENT ON NRPDTA.NRP10F1 (FRENOVACION is 'Fecha de renovacion');

ALTER TABLE NRPDTA.NRP10F1 ADD COLUMN PENDEUDAMIENTO  DECIMAL(2);
COMMENT ON NRPDTA.NRP10F1 (PENDEUDAMIENTO is 'Porcentaje Endeudamiento');

ALTER TABLE NRPDTA.NRP10F1 ADD COLUMN NOMDEU VARCHAR(100);
COMMENT ON NRPDTA.NRP10F1 (NOMDEU is 'Nombre Deudor');

ALTER TABLE NRPDTA.NRP10F1 ADD COLUMN APEPATDEU VARCHAR(50);
COMMENT ON NRPDTA.NRP10F1 (APEPATDEU is 'Apellido Paterno Deudor');

ALTER TABLE NRPDTA.NRP10F1 ADD APEMATDEU VARCHAR(50);
COMMENT ON NRPDTA.NRP10F1 (APEMATDEU is 'Apellido Materno Deudor');

ALTER TABLE NRPDTA.NRP10F1 ADD COLUMN RENTAS DECIMAL(12);
COMMENT ON NRPDTA.NRP10F1 (RENTAS is 'Rentas');

ALTER TABLE NRPDTA.NRP10F1 ADD COLUMN FULVCTOCRE VARCHAR(8);
COMMENT ON NRPDTA.NRP10F1 (FULVCTOCRE is 'Fecha vencimiento credito');

ALTER TABLE NRPDTA.NRP10F1 ADD COLUMN NUMCREORI VARCHAR(20);
COMMENT ON NRPDTA.NRP10F1 (NUMCREORI is 'Numero credito original');





/* NRP15F1 */

