COMMENT ON NRPDTA.NRP15F1 (CELECTRONICO is 'Correo Electronico Empresa');

ALTER TABLE NRPDTA.NRP15F1 ADD COLUMN NUMCREORI  VARCHAR(20);
COMMENT ON NRPDTA.NRP15F1 (NUMCREORI is 'Numero Credito Original');

ALTER TABLE NRPDTA.NRP15F1 ADD COLUMN FGENERACION  VARCHAR(10);
COMMENT ON NRPDTA.NRP15F1 (FGENERACION is 'Fecha Generacion Proceso');

