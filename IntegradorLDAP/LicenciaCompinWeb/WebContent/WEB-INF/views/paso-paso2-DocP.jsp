				<form class="form" id="paso2" action="paso3DocP.do" method="POST">
					
					<div class="row">
						<div class="col xs12 lg6">
							<div class="form__grupo" data-animacion="placeholder"
								data-comentario="">
								<input class="text requerido" type="text" id="rut" name="rut"
