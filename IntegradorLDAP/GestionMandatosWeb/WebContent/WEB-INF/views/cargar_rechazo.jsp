	<div class="col xs12 lg10">
		<p style="font-size:large;">Mandatos - Subir Archivo Rechazos</p>
		<div class="separador"></div>
		<form class="form" id="subirRechazo" action="subirRechazo.do" method="post" enctype="multipart/form-data" >
				<div class="row">
					<div class="col xs12 lg6">

						
						<div class="form__grupo" data-animacion="placeholder"
							data-comentario="Archivo csv o txt" style="font-size: medium;">
							<input
							class="documento__input" type="file" id="rechazo" name="rechazo"
							data-maxsize="5000"
							accept=".csv, text/plain" />
							<input type="hidden" name="nombre" id="nombre" value="ddd"/>
						</div>
						
						<div class="separador"></div>
						 <div class="alerta alerta--error" id="errores_paso1" style="display:none"></div>
						<div class="btn__grupo text-align-right-xs">
							<a class="btn btn--secundario" href="<c:url value='/ejecutivo.do' />" >Volver</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
							<a class="btn btn--primario" href="#" onClick="validarRechazo();">Subir</a>
							
						</div>

					</div>
				</div>
				
			</form>
	</div>
	<div class="col xs12 lg10">
	<c:if test="${resumen!=null}">
	<table align="center" class="tabla-creditos" style="width: 400px;">
		<thead>
			<tr>
				<th class="certificadoCenter" colspan="2" style="font-size: 16px; height: 30px">Resumen de Carga Archivo Rechazos</th>

			</tr>
		</thead>
		<tbody>
		
				<tr>
					<td class="certificadoLeft" >Cantidad de Registros Válidos: </td>
					<td class="certificadoLeft"><fmt:formatNumber type="number"
							maxFractionDigits="0" value="${resumen.cantidadRegistros}" /></td>
				</tr>	
		
				<c:if test="${resumen.listaErrores.size()>0}">
				<tr>
					<td class="certificadoLeft">Con Errores:</td>
					<td class="certificadoLeft"><fmt:formatNumber type="number"
							maxFractionDigits="0" value="${resumen.listaErrores.size()}" />
							
							&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a href="<c:url value='/conerrores.do' />"><input type="button" name="descargar" value="descargar"/></a>
							
							</td>
				</tr>
				</c:if>

		</tbody>

	</table>
	</c:if>	
