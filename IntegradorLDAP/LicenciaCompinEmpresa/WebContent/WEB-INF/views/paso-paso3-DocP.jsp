	<script type="text/javascript">
		$('#region').change(
				function() {
					$.getJSON('comunas.do', {
						regionNombre : $(this).val(),
						ajax : 'true'
					}, function(data) {
						var html = '';
						var len = data.length;
						for (var i = 0; i < len; i++) {

							html += '<option value="' + data[i].id + '">'
									+ data[i].descripcion + '</option>';
						}
						html += '</option>';
						$('#comuna').html(html);
					});
				});
	</script>
	<c:if test='${mensaje=="1"}'>
	<script>
		avisar('Hemos recepcionado tu documento. Puedes enviar otro o finalizar.', 5000);
	</script>
	</c:if>
