	<script type="text/javascript">
		$('#region').change(
				function() {
					$.getJSON('comunas.do', {
						regionNombre : $(this).val(),
						ajax : 'true'
					}, function(data) {
						var html = '';
						var len = data.length;
						for (var i = 0; i < len; i++) {

							html += '<option value="' + data[i].id + '">'
									+ data[i].descripcion + '</option>';
						}
						html += '</option>';
						$('#comuna').html(html);
					});
				});
	</script>
	<script>
	$(document).ready(function() {
			$('#folioLicencia').keyup(function(){
        		$(this).val($(this).val().toUpperCase());
    		});
		});
	</script>
