
CREATE TABLE EMPRESA ( 
	ID_EMPRESA DECIMAL(9, 0) , 
	ID_CASA_MATRIZ CHAR(6), 
	ID_ADMIN DECIMAL(9, 0), 
	ID_REP_LEGAL DECIMAL(9, 0), 
	RAZON_SOCIAL CHAR(100)  , 
	PRIVADA SMALLINT DEFAULT 1 , 
	HABILITADA SMALLINT DEFAULT 1 , 
	INICIACION DATE , 
	VIGENCIA_REP_LEGAL  DATE  , 
	NUM_TRABAJADORES INTEGER , 
	ULT_PERIODO_COTIZADO  NUMERIC(6, 0)  , 
	TIPO VARCHAR(13) DEFAULT 'EMPRESA' , 
	TIPO_PAGO CHAR(15)  
; 
  

