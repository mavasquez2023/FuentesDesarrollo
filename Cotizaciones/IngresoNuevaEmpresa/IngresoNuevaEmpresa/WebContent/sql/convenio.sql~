	  
CREATE TABLE CONVENIO ( 
	ID_EMPRESA DECIMAL(9, 0)  , 
	ID_CONVENIO  SMALLINT  , 
	ID_GRUPO_CONVENIOS  INTEGER  , 
	ID_MAPANOM_REM INTEGER DEFAULT NULL , 
	ID_MAPANOM_GRA INTEGER DEFAULT NULL , 
	ID_MAPANOM_DEP INTEGER DEFAULT NULL , 
	ID_MAPANOM_REL INTEGER DEFAULT NULL , 
	ID_MAPACOD INTEGER DEFAULT NULL , 
	ID_OPCION INTEGER DEFAULT NULL , 
	ID_CCAF SMALLINT DEFAULT NULL , 
	ID_MUTUAL SMALLINT  , 
	ID_ACTIVIDAD DECIMAL(6, 0) DEFAULT NULL , 
	CALCULO_MOV_PERSONAL SMALLINT  , 
	HABILITADO SMALLINT  DEFAULT 1 , 
	CREADO TIMESTAMP  , 
	ULTIMO_USO TIMESTAMP  , 
	MUTUAL_NUMERO_ADHERENTE INTEGER  , 
	MUTUAL_TASA_ADICIONAL DECIMAL(6, 4)  , 
	MUTUAL_CALCULO_INDIVIDUAL SMALLINT  DEFAULT 0 , 
	N_NOMINAS INTEGER  DEFAULT 0 , 
	N_NOMINAS_OK INTEGER  DEFAULT 0 , 
	N_NOMINAS_CORREGIDAS INTEGER  DEFAULT 0 , 
	N_COTIZACIONES INTEGER  DEFAULT 0 , 
	N_COTIZACIONES_OK INTEGER  DEFAULT 0 , 
	N_COTIZACIONES_CORREGIDAS INTEGER  DEFAULT 0 , 
	N_BLOQUEOS SMALLINT  DEFAULT 0 , 
	DESCRIPCION FOR COLUMN DESCR00001 CHAR(30)  DEFAULT ''  ) ; 

