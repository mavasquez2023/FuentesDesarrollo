CREATE TABLE GRUPOCONV ( 
	ID_GRUPO_CONVENIOS INTEGER  , 
	ID_EMPRESA DECIMAL(9, 0)  , 
	ID_MAPANOM_REM INTEGER  DEFAULT 0 , 
	ID_MAPANOM_GRA INTEGER  DEFAULT 0 , 
	ID_MAPANOM_DEP INTEGER  DEFAULT 0 , 
	ID_MAPANOM_REL INTEGER  DEFAULT 0 , 
	ID_MAPACOD INTEGER  DEFAULT 0 , 
	ID_OPCION INTEGER  DEFAULT 0 , 
	NOMBRE CHAR(30)  , 
	CREADO TIMESTAMP  , 
	HABILITADO SMALLINT  DEFAULT 1 , 
	N_BLOQUEOS SMALLINT  DEFAULT 0 , 
	PREVIRED SMALLINT  DEFAULT 0 , 
	CONFIG_BASE SMALLINT  DEFAULT 0 ) ; 
  
