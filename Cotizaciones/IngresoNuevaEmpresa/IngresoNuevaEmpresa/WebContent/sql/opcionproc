CREATE TABLE OPCIONPROC ( 
	ID_OPCION INTEGER ,
	GENERAR_INP_POR_SUCURSAL SMALLINT NOT NULL DEFAULT 0 , 
	GENERAR_MUTUAL_POR_SUCURSAL SMALLINT NOT NULL DEFAULT 0 , 
	GENERAR_CCAF_POR_SUCURSAL SMALLINT NOT NULL DEFAULT 0 , 
	CALCULAR_MONTO_INP SMALLINT NOT NULL DEFAULT 0 , 
	CALCULAR_MONTO_MUTUAL SMALLINT NOT NULL DEFAULT 0 , 
	CALCULAR_MONTO_CCAF SMALLINT NOT NULL DEFAULT 0 , 
	CALCULAR_MONTO_TOTAL_SALUD SMALLINT NOT NULL DEFAULT 0 , 
	CALCULAR_MONTO_TOTAL_PREVISION  SMALLINT NOT NULL DEFAULT 0 , 
	IMPRIMIR_PLANILLAS SMALLINT NOT NULL DEFAULT 0 , 
	N_BLOQUEOS SMALLINT NOT NULL DEFAULT 0 , 
	CALCULAR_MONTO_FONASA SMALLINT NOT NULL DEFAULT 0  ) ; 
