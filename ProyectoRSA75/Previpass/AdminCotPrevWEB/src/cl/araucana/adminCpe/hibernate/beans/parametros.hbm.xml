<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cl.araucana.cp.distribuidor.hibernate.beans.ParametroVO" table="PARAMETRO">
        <id name="id" type="int" column="ID_PARAMETRO"/>
        <property name="nombre" column="NOMBRE" />
        <property name="descripcion" column="DESCRIPCION" />
        <property name="valor" column="VALOR" />
        <property name="tipoParametro" column="TIPO_PARAMETRO" />
        <property name="tipoValor" column="TIPO_VALOR" />
    </class>
</hibernate-mapping>
