<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cl.araucana.cp.hibernate.beans">
    <class name="cl.araucana.cp.distribuidor.hibernate.beans.AsigFamVO" table="TRAMOASFAM">
    	<id name="id" column="ID_TRAMO">
    		<generator class="increment"/>
   		</id>
   		<property name="nombre" column="NOMBRE" />
   		<property name="rentaMinima" column="RENTA_MINIMA" />
   		<property name="rentaMaxima" column="RENTA_MAXIMA" />
   		<property name="valorCarga" column="VALOR_CARGA_FAMILIAR" />
    </class>
</hibernate-mapping>
