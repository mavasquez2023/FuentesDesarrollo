<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cl.araucana.cp.distribuidor.hibernate.beans.CalendarioVO" table="CALENDARIO">
    	<id name="idCal" column="ID_CALENDARIO"></id>
    	<property name="mes" column="MES" />
   		<property name="fecha1" column="FECHA1" />
   		<property name="fecha2" column="FECHA2" />
   		<property name="fecha3" column="FECHA3" />
   		<property name="fecha4" column="FECHA_CIERRE" /> 
   		<property name="informacion" column="INFORMACION" />
    </class>
</hibernate-mapping>
