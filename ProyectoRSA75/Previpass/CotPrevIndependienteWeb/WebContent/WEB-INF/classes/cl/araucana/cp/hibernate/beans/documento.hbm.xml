<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cl.araucana.cp.hibernate.beans.DocumentoVO" table="DOCUMENTO">
    	<id column="ID_DOCUMENTO" name="id">
   			<generator class="sequence">
                <param name="sequence">S_DOCUMENTO</param>
            </generator>
   		</id>
   		<property name="idEnvio" column="ID_ENVIO" />
   		<property name="rutEmpresa" column="EMPRESA" />
   		<property name="idConvenio" column="CONVENIO"/>
   		<property name="tipoProceso" column="TIPO_NOMINA"/>
   		<property name="nombre" column="NOMBRE" />
   		<property name="data" column="DATA"/>
    </class>
</hibernate-mapping>
