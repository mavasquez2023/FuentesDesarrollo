<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd">
    <diskStore path="java.io.tmpdir"/>
    <defaultCache
    	name="cacheWeb"
        maxElementsInMemory="10000"
        eternal="true"
        timeToIdleSeconds="120"
        timeToLiveSeconds="120"
        overflowToDisk="true"
        diskPersistent="false"
        diskExpiryThreadIntervalSeconds="120"
        memoryStoreEvictionPolicy="LRU"/>       
    <cache name="cl.araucana.cp.distribuidor.hibernate.beans.EstadoEnvioVO" maxElementsInMemory="300" eternal="true" overflowToDisk="false"/>
    <cache name="cl.araucana.cp.distribuidor.hibernate.beans.EstadoEdicionVO" maxElementsInMemory="300" eternal="true" overflowToDisk="false"/>
    <cache name="cl.araucana.cp.distribuidor.hibernate.beans.EstadoNominaVO" maxElementsInMemory="300" eternal="true" overflowToDisk="false"/>
    <cache name="cl.araucana.cp.distribuidor.hibernate.beans.TipoNominaVO" maxElementsInMemory="300" eternal="true" overflowToDisk="false"/>
    <cache name="cl.araucana.cp.distribuidor.hibernate.beans.ParametroVO" maxElementsInMemory="300" eternal="true" overflowToDisk="false"/>
</ehcache>
