<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cl.araucana.cp.distribuidor.hibernate.beans.LectorEmpresaConvenioVO" table="LECCONVEN">
    	<composite-id >
        	<key-property name="idLectorEmpresa">
        		<column name="ID_LECTOR_EMPRESA" not-null="true" />
        	</key-property>
        	<key-property name="idEmpresa">
        		<column name="ID_EMPRESA" not-null="true" />
        	</key-property>
        	<key-property name="idConvenio">
        		<column name="ID_CONVENIO" not-null="true" />
        	</key-property>
    	</composite-id>
    </class>
</hibernate-mapping>
