<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cl.araucana.cp.distribuidor.hibernate.beans.AvisosCotizanteVO" table="AVISOS_COTIZANTE">
    	<composite-id>
        	<key-property name="rutEmpresa">
        		<column name="ID_EMPRESA" not-null="true"/> 
        	</key-property>
        	<key-property name="idConvenio">
        		<column name="ID_CONVENIO" not-null="true"/> 
        	</key-property>
        	<key-property name="idCotizante">
        		<column name="ID_COTIZANTE" not-null="true"/> 
        	</key-property>
        	<key-property name="periodo">
        		<column name="PERIODO" not-null="true"/> 
        	</key-property>
    	</composite-id>
   	</class>
</hibernate-mapping>
