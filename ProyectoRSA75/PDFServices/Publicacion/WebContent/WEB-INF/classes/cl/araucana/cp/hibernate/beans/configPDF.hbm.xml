		<property name="numTrabM9" column="N_TRAB_M9" />
		<property name="numTrabM10" column="N_TRAB_M10" />
		<property name="numTrabM11" column="N_TRAB_M11" />
		<property name="numTrabM12" column="N_TRAB_M12" />
		<property name="mxAsterisco" column="MX_ASTERISCO" />
		<property name="mxNoMostrar" column="MX_NO_MOSTRAR" />
	</class>
</hibernate-mapping>
